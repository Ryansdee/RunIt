{
  "expo": {
    "name": "RunIt",
    "slug": "RunIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://i.ibb.co/VNWjz3V/runit-3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "userInterfaceStyle": "light"
      },
      "package": "RunIt.Ryansdee"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "userInterfaceStyle": "light",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f3a29adc-44c5-4187-acc7-6e9f16c56434"
      }
    }
  }
}
